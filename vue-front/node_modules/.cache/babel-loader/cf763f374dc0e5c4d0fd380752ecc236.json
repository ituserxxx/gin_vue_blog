nt=\"5\"\n        layout=\"prev, pager, next\"\n        :total=\"total\"\n        :hide-on-single-page=\"true\"\n         @current-change=\"handleCurrentChange\"\n      >\n      </el-pagination>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { articleListApi } from \"../api/article.js\";\nexport default {\n  name: \"ContentContainer\",\n  data() {\n    return {\n      current_page: 1,\n      total: 0,\n      article_list: [],\n      // is_empty:true,\n    };\n  },\n  created() {\n    this.initArticleList();\n  },\n  methods: {\n    initArticleList() {\n      this.randArticleList();\n    },\n    handleCurrentChange(val) {\n      this.current_page = val;\n      this.randArticleList()\n    },\n    gotoDetail(id) {\n      this.$router.push({\n        path: \"/Detail\",\n        query: {\n          id: id,\n        },\n      });\n    },\n\n    randArticleList() {\n      articleListApi({\n        page: this.current_page,\n      })\n        .then((res) => {\n          this.article_list = res.data.article_list;\n          this.total = res.data.total;\n        })\n        .catch((err) => {\n          console.log(err);\n        });\n    },\n  },\n};\n</script>\n\n\n<style  scoped>\n.Page-tool {\n  text-align: center;\n}\n.ArticleListContainer {\n  margin-top: 50px;\n  background-color: #e9eef3;\n  color: #333;\n  \n}\n.ArticleListContainer li {\n  list-style-type: none;\n}\n.list li {\n  margin: 0% 20% 0px 20%;\n  text-align: center;\n  width: 60%;\n}\n</style>"],"mappings":";AA+BA,SAASA,cAAa,QAAS,mBAAmB;AAClD,eAAe;EACbC,IAAI,EAAE,kBAAkB;EACxBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE,CAAC;MACfC,KAAK,EAAE,CAAC;MACRC,YAAY,EAAE;MACd;IACF,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,eAAe,CAAC,CAAC;EACxB,CAAC;EACDC,OAAO,EAAE;IACPD,eAAeA,CAAA,EAAG;MAChB,IAAI,CAACE,eAAe,CAAC,CAAC;IACxB,CAAC;IACDC,mBAAmBA,CAACC,GAAG,EAAE;MACvB,IAAI,CAACR,YAAW,GAAIQ,GAAG;MACvB,IAAI,CAACF,eAAe,CAAC;IACvB,CAAC;IACDG,UAAUA,CAACC,EAAE,EAAE;MACb,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC;QAChBC,IAAI