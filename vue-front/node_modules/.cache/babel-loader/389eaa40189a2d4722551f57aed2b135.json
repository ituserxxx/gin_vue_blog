=\"index\"\n        :value=\"val.article_sum\"\n        :max=\"50\"\n      >\n        <el-button type=\"success\" @click=\"queryTagArticle(val.id)\">\n          {{ val.tag_name }}\n        </el-button>\n      </el-badge>\n    </div>\n    <br />\n    <div class=\"tag_article_list\">\n      <el-timeline-item\n        v-for=\"(activity, index) in article_list\"\n        :key=\"index\"\n        :hide-timestamp=\"true\"\n        @click=\"gotoDetail(activity.id)\"\n      >\n        <el-card>\n          <h4>{{ activity.title }}</h4>\n        </el-card>\n      </el-timeline-item>\n    </div> \n  </el-main>\n</template>\n<script>\nimport { tagListApi, tagArticleListApi } from \"../api/tag.js\";\n\nexport default {\n  name: \"Tag\",\n\n  data() {\n    return {\n      tag_list: [],\n      article_list: [],\n      article_page: 1,\n    };\n  },\n\n  created() {\n    this.initTagListApi();\n    const id = this.$route.query.id;\n    if (id != undefined) {\n      this.initTagArticleList();\n    }\n  },\n  watch: {\n    $route() {\n      this.initTagArticleList();\n    },\n  },\n  methods: {\n    initTagListApi() {\n      tagListApi({ page: -1 })\n        .then((res) => {\n          this.tag_list = res.data;\n        })\n        .catch((err) => {\n          console.log(err);\n        });\n    },\n    initTagArticleList() {\n      const id = this.$route.query.id;\n      if (id != undefined) {\n        tagArticleListApi({ id: Number(id), page: this.article_page })\n          .then((res) => {\n            this.article_list = res.data.article_list;\n          })\n          .catch((err) => {\n            console.log(err);\n          });\n      }\n    },\n    queryTagArticle(id) {\n      this.$router.push({\n        path: \"/Tag\",\n        query: {\n          id: id,\n        },\n      });\n    },\n    gotoDetail(id) {\n      this.$router.push({\n        path: \"/Detail\",\n        query: {\n          id: id,\n        },\n      });\n    },\n  },\n};\n</script>\n\n<style scoped>\n.Tag{\n    margin-top: 50px;\n}\n.tag_list {\n  margin: 0% 20% 10% 20%;\n}\n.tag_article_list {\n  text-align: left;\n}\n.item {\n  margin-top: 10px;\n  margin-right: 40px;\n}\n.Tag >>> .el-card__body {\n  padding-top: 0px;\n  padding-bottom: 0px;\n}\n\n.tag_article_list li {\n  list-style-type: none;\n    margin: 0% 20% 0px 20%;\n  text-align: center;\n  width: 60%;\n}\n</style>\n"],"mappings":";;;EAESA,KAAK,EAAC;AAAU;gEAarBC,mBAAA,CAAM;;EACDD,KAAK,EAAC;AAAkB;;;;;;;uBAf/BE,YAAA,CA2BUC,kBAAA;IA3BDH,KAAK,EAAC;EAAK;sBAClB,MAYM,CAZNC,mBAAA,CAYM,OAZNG,UAYM,I,kBAXJC,mBAAA,CAUWC,SAAA,QAAAC,WAAA,CARcC,KAAA,CAAAC,QAAQ,GAAvBC,GAAG,EAAEC,KAAK;2BAFpBT,YAAA,CAUWU,mBAAA;QATTZ,KAAK,EAAC,MAAM;QAEXa,GAAG,EAAEF,KAAK;QACVG,KAAK,EAAEJ,GAAG,CAACK,WAAW;QACtBC,GAAG,EAAE;;0BAEN,MAEY,CAFZC,YAAA,CAEYC,oBAAA;UAFDC,IAAI,EAAC,SAAS;UAAEC,OAAK,EAAAC,MAAA,IAAEC,QAAA,CAAAC,eAAe,CAACb,GAAG,CAACc,EAAE;;4BACtD,MAAkB,C,kCAAfd,GAAG,CAACe,QAAQ,iB;;;;;sCAIrBC,UAAM,EACNzB,mBAAA,CAWM,OAXN0B,UAWM,I,kBAVJtB,mBAAA,CASmBC,SAAA,QAAAC,WAAA,CARWC,KAAA,CAAAoB,YAAY,GAAhCC,QAAQ,EAAElB,KAAK;2BADzBT,YAAA,CASmB4B,2BAAA;QAPhBjB,GAAG,EAAEF,KAAK;QACV,gBAAc,EAAE,IAAI;QACpBS,OAAK,EAAAC,MAAA,IAAEC,QAAA,CAAAS,UAAU,CAACF,QAAQ,CAACL,EAAE;;0BAE9B,MAEU,CAFVP,YAAA,CAEUe,kBAAA;4BADR,MAA6B,CAA7B/B,mBAAA,CAA6B,YAAAgC,gBAAA,CAAtBJ,QAAQ,CAACK,KAAK,iB","ignoreList":[]}]}